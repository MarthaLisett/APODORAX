programa Martha0295:

var entero result;
var entero term1;
var entero term2;
var entero i;
var entero num;

inicio {
	result = 1;
	term1 =  0;
	term2 = 1;
	i = 1;
	entrada(num);
	mientras (i < num){
		result = term1 + term2;
		term1 = term2;
		term2 = result;
		i = i + 1 ;
	}
	desplegar (result);
}

fin