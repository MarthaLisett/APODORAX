programa Martha0295:

var entero res;
var entero num;

funcion entero factorial(var entero x) {
	var entero resultado;
	var entero aux;

	si (x <= 1) entonces {
		resultado = 1;
	} sino entonces {
		aux = factorial(x - 1);
		resultado = aux * x;
	}
	regresar resultado;
}

inicio {
	entrada(num);
	res = factorial(num);
	desplegar(res);
}

fin